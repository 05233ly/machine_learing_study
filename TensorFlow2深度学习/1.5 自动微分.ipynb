   "source": [
    "import tensorflow as tf \n",
    "\n",
    "# 创建4个张量\n",
    "a = tf.constant(1.)\n",
    "b = tf.constant(2.)\n",
    "c = tf.constant(3.)\n",
    "w = tf.constant(4.)\n",
    "\n",
    "\n",
    "with tf.GradientTape() as tape:# 构建梯度环境\n",
    "    tape.watch([w]) # 将w加入梯度跟踪列表\n",
    "    # 构建计算过程\n",
    "    y = a * w**2 + b * w + c\n",
    "# 求导\n",
    "[dy_dw] = tape.gradient(y, [w])\n",
    "print(dy_dw)\n",
    "\n"
   ]
  },
